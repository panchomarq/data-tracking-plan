# Amplitude Taxonomy API credentials
# Get these from: Amplitude > Settings > Projects > [Your Project] > API Keys
AMPLITUDE_API_KEY=your_api_key_here
AMPLITUDE_SECRET_KEY=your_secret_key_here

# Region: "us" (default) or "eu"
AMPLITUDE_REGION=us

# Source mode: "api" (live Taxonomy API) or "csv" (local CSV fallback)
AMPLITUDE_SOURCE_MODE=api

# Cache TTL in seconds (default 900 = 15 minutes)
AMPLITUDE_CACHE_TTL=900
